{
  "groups": [
    {
      "title": "General Components",
      "id": "general",
      "components": [
        {
          "title": "Title",
          "id": "title",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/title",
                "template": {
                  "title": "Title",
                  "type": "h1",
                  "weight": "normal",
                  "color": "black",
                  "text-transform": "none",
                  "vertical-spacing": "none"
                }
              }
            }
          }
        },
        {
          "title": "Link",
          "id": "link",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/link",
                "template": {
                  "label": "Link",
                  "href": "#",
                  "openInNewTab": true,
                  "type": "link",
                  "link-variant": "default",
                  "button-type": "default",
                  "fullWidth": false
                }
              }
            }
          }
        },
        {
          "title": "Text",
          "id": "text",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/text",
                "template": {
                  "text": "Text",
                  "text-size": "base",
                  "weight": "normal",
                  "color": "black",
                  "text-transform": "none",
                  "vertical-spacing": "none"
                }
              }
            }
          }
        },
        {
          "title": "Richtext",
          "id": "richtext",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/richtext",
                "template": {
                  "text": "Richtext",
                  "color": "black",
                  "vertical-spacing": "none"
                }
              }
            }
          }
        }
      ]
    },
    {
      "title": "Container Components",
      "id": "containers",
      "components": [
        {
          "title": "Flex Container",
          "id": "flexcontainer",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/flexcontainer",
                "template": {
                  "flex-direction": "row",
                  "flex-wrap": "nowrap",
                  "flex-align": "stretch",
                  "flex-justify": "flexStart",
                  "item-gap": "none",
                  "vertical-spacing": "none"
                }
              }
            }
          }
        },
        {
          "title": "Grid Container",
          "id": "gridcontainer",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/gridcontainer",
                "template": {
                  "ideal-column-size": "20rem",
                  "max-column-count": 3,
                  "item-gap": "0rem"
                }
              }
            }
          }
        },
        {
          "title": "Pricing Card",
          "id": "pricingcard",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/pricingcard",
                "template": {}
              }
            }
          }
        }
      ]
    }
  ]
}
