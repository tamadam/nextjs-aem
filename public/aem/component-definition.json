{
  "groups": [
    {
      "title": "General Components",
      "id": "general",
      "components": [
        {
          "title": "Title",
          "id": "title",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/title",
                "template": {
                  "title": "Title",
                  "type": "h1",
                  "weight": "normal",
                  "color": "black",
                  "text-transform": "none",
                  "vertical-spacing": "none"
                }
              }
            }
          }
        },
        {
          "title": "Richtext",
          "id": "richtext",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/richtext",
                "template": {
                  "text": "Richtext",
                  "color": "black"
                }
              }
            }
          }
        },
        {
          "title": "FlexContainer",
          "id": "flexcontainer",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/flexcontainer",
                "template": {
                  "flex-direction": "row",
                  "flex-wrap": "nowrap",
                  "flex-align": "stretch",
                  "flex-justify": "flexStart",
                  "item-gap": "none",
                  "vertical-spacing": "none"
                }
              }
            }
          }
        }
      ]
    },
    {
      "title": "Advanced Components",
      "id": "advanced",
      "components": [
        {
          "title": "Link",
          "id": "link",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/link",
                "template": {
                  "label": "Link",
                  "href": "#",
                  "openInNewTab": true,
                  "type": "link",
                  "link-variant": "default",
                  "button-type": "default"
                }
              }
            }
          }
        }
      ]
    }
  ]
}
