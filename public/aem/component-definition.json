{
  "groups": [
    {
      "title": "General Components",
      "id": "general",
      "components": [
        {
          "title": "Title Component",
          "id": "title",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/title",
                "template": {
                  "title": "Title",
                  "type": "h1",
                  "weight": "normal",
                  "color": "black",
                  "text-transform": "none"
                }
              }
            }
          }
        },
        {
          "title": "RichText Component",
          "id": "richtext",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/text",
                "template": {
                  "text": "Richtext default value"
                }
              }
            }
          }
        }
      ]
    },
    {
      "title": "Advanced Components",
      "id": "advanced",
      "components": [
        {
          "title": "Link",
          "id": "link",
          "plugins": {
            "aem": {
              "page": {
                "resourceType": "demo-app/components/link",
                "template": {
                  "label": "Link",
                  "href": "#",
                  "openInNewTab": true,
                  "type": "link",
                  "link-color": "green",
                  "link-variant": "default",
                  "button-type": "primary"
                }
              }
            }
          }
        }
      ]
    }
  ]
}
